{% extends 'layout.html.twig' %}

{% block content %}
    <div class="container-fluid">
      <ul class="deck">
      {% for i in random12Egg %}
        {% for egg in i%}
        <li class="card {{ egg.rarity }} w-12">
          <img src="{{ egg.image }}" class="card-img-top w-80" alt="...">
          <h5 class="card-title text-center">{{ egg.name }}</h5>
        </li>
      {% endfor %}
    {% endfor %}
      </ul>
    </div>
{% endblock %}
